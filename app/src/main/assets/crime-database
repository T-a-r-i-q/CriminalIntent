DROP TABLE IF EXISTS Crime;

-- Create table with correct types
CREATE TABLE Crime (
    id TEXT PRIMARY KEY,
    title TEXT NOT NULL,
    date INTEGER NOT NULL,
    isSolved INTEGER NOT NULL
);

